{
  "name": "asciisd/zoho",
  "description": "",
  "keywords": ["Laravel"],
  "authors": [
    {
      "name": "Amr Ahmed",
      "email": "aemad@asciisd.com"
    }
  ],
  "homepage": "https://github.com/asciisd/zoho",
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "5.8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^4.4",
    "phpstan/phpstan": "^0.12",
    "zohocrm/php-sdk": "^2.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Asciisd\\Zoho\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asciisd\\Zoho\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Asciisd\\Zoho\\Providers\\ZohoServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
    "analysis": "vendor/bin/phpstan analyse"
  }
}
